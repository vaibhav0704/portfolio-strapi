{
  "kind": "collectionType",
  "collectionName": "page_infos",
  "info": {
    "singularName": "page-info",
    "pluralName": "page-infos",
    "displayName": "pageInfo",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "role": {
      "type": "string"
    },
    "heroImage": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "backgroundInformation": {
      "type": "richtext"
    },
    "profilePic": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "phoneNumber": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "address": {
      "type": "richtext"
    },
    "socials": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::social.social"
    }
  }
}
